<div class="container-fluid callback my-5 pt-5">
    <div class="container pt-5">
        <div class="row justify-content-center">
            <div class="col-lg-7">
                <div class="bg-white border rounded p-4 p-sm-5 wow fadeInUp" data-wow-delay="0.5s">
                    <div class="text-center mx-auto wow fadeInUp" data-wow-delay="0.1s" style="max-width: 600px;">
                        <p class="d-inline-block border rounded text-primary fw-semi-bold py-1 px-3">Get In Touch</p>
                        <h1 class="display-5 mb-5">Request A Call-Back</h1>
                    </div>

                    <div class="row g-3">
                        @if (@Model.Item4 != null)
                        {
                            <form asp-action="Index" asp-controller="Home" target="_self" enctype="multipart/form-data">
                                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                                <div style="display: flex;">
                                    <div class="col-sm-6" style=" margin-right: 10px;  margin-bottom: 10px;     width: 49%;">
                                         <div class="form-floating">
                                        <input href="Name"  type="text" class="form-control" id="name" placeholder="Your Name" name="Name"  required/>
                                        <label for="Name" >Your Name</label>
                                        </div>
                                    </div>
                                    <div class="col-sm-6">
                                        <div class="form-floating">
                                        <input href="Email"  type="email" class="form-control" id="mail" placeholder="Your Email" name="Email"  required/>
                                        <label for="Email" >Your Email</label>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-sm-12"  style="margin-bottom: 10px;">
                                    <div class="form-floating">
                                     <input href="PhoneNumber"  type="tel" class="form-control" id="mobile" placeholder="Your PhoneNumber" name="PhoneNumber" style="    padding: 1rem 1rem"  required/>
                                    <label for="PhoneNumber" >Your PhoneNumber</label>
                                    </div>
                                </div>

                                <div class="col-12">
                                    <div class="form-floating" style="margin-bottom: 15px;">
                                  <textarea for="Message" href="Message" type="text" id="message" class="form-control" placeholder="Your Message" name="Message" style="height: 100px; padding: 1rem 1rem"  required></textarea>
                                    <label for="Message">Message</label>
                                    </div>
                                </div>
                                <div class="col-12 text-center">
                                    @*<input type="submit" value="Create" class="btn btn-primary" />*@
                                 <button class="btn btn-primary w-100 py-3" type="submit">Submit Now</button>
                                </div>
                            </form>
                        }
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Callback End -->

