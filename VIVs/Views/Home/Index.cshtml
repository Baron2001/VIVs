                    {
                        <div class="testimonial-item">
                            <div class="testimonial-text border rounded p-4 pt-5 mb-5">
                                <div class="btn-square bg-white border rounded-circle" style="display:grid;">
                                    @*<i class="fa fa-quote-right fa-2x text-primary"></i>*@
                                    <div><b>@item.Numberofitem</b></div>
                                    <p>Items</p>
                                </div>
                                <b>@item.Description</b>

                            </div>
                            <img class="rounded-circle mb-3" src="@Url.Content("~/Images/"+@item.Users.Image)"  alt="">
                            <h4>@item.Users.Fullname</h4>
                            @{
                                switch(@item.Users.Categorytypeid)
                                {
                                    case 1:
                                        // Use the text block below to separate html elements from code
                                                        <span>Medical Clinics</span>
                                        //<text>
                                        //    <h1>Test Site</h1>
                                        //</text>
                                        break;  // Always break each case
                                    case 2:
                                               <text>
                                                   <span>Restaurants</span>
                                               </text>
                                        break;
                                    case 3:
                                               <text>
                                                   <span>Pharmacies</span>
                                               </text>
                                        break;
                                    default:
                                               <text>
                                                   <span>Other Volunteering Services</span>
                                               </text>
                                        break;
                                }
                            }
                            <br/>
                            @if(@name !=null)
                            {
                                @if(@item.Numberofitem>0)
                                {
                                    <a asp-action="Login" asp-controller="Auth"  class="btn btn-primary " style="margin-top:5px;">
                                        Book
                                    </a>
                                }
                                else
                                {
                                    <a class="btn btn-danger " style="margin-top:5px;">
                                        Out of Stock
                                    </a>
                                }
                            }
                            else 
                            {
                                <a asp-action="Login" asp-controller="Auth"  class="btn btn-warning " style="margin-top:5px;">
                                    Login For Booking
                                </a>
                            }
                            
                        </div>
                    }
                }
